{
  "manifest_version": "1",

  "name": "Volsbb Autologin",
  "description": "This extension helps to auto login Volsbb",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
